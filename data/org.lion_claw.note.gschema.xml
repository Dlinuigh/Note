<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="note">
	<schema id="org.lion_claw.note" path="/org/lion_claw/note/">
		<key name='height' type='u'><default>800</default></key>
		<key name='width' type='u'><default>600</default></key>
		<key name='title-num' type='u'><default>10</default></key>
		<key name='subtitle-num' type='u'><default>20</default></key>
		<key name='subtitle-line' type='u'><default>1</default></key>
		<key name='custom-window-size' type='b'><default>true</default></key>
		<key name='custom-font' type='b'><default>true</default></key>
		<key name='custom-editor-font' type='b'><default>true</default></key>
		<key name='custom-font-border' type='s'><default>'Cantarell Normal Regular 11'</default></key>
		<key name='custom-font-title' type='s'><default>'Cantarell Normal Regular 11'</default></key>
		<key name='custom-font-subtitle' type='s'><default>'Cantarell Normal Regular 11'</default></key>
		<key name='custom-font-editor' type='s'><default>'Cantarell Normal Regular 11'</default></key>
		<key name='lock-time' type='i'><default>6</default></key>
	</schema>
</schemalist>
